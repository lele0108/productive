{
  "name": "Productive",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Become more productive",
  "homepage_url": "http://jliu.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
          "storage",
          "alarms"
  ],
  "default_locale": "en",
  "background": {
    "scripts": ["src/bg/jquery.js","src/bg/eventPage.js"],
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "content_security_policy": "script-src 'self' https://todoist.com; object-src 'self'"
}